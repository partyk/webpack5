{
  "//": "TODO vue-loader because of Vue 2 I have to use version 15.x for VUE 3 version > 16.x will be used",
  "name": "webpack5",
  "version": "1.0.0",
  "description": "DevSteck webpack 5",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "beforeBuild": "npm run cleanUp && npm run generateIconFont",
    "build": "webpack --env production --config=./webpack/config/webpack.prod.js",
    "cleanUp": "node webpack/script/cleanUp.js",
    "download": "node webpack/script/download.js",
    "dev": "webpack --config=./webpack/config/webpack.dev.js",
    "dev:server": "webpack-dev-server --config=./webpack/config/webpack.devServer.js",
    "start": "npm run dev",
    "predev": "npm run beforeBuild",
    "predev:build": "npm run beforeBuild",
    "predev:server": "npm run beforeBuild",
    "generateIconFont": "fantasticon src/icons -o temp/webpack/fonts/icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/partyk/webpack5.git"
